{% extends 'layoutFrontOffice.html.twig' %}


{% block title %}Oeuvre index{% endblock %}

{% block body %}
    <h1>Oeuvre index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Type</th>
                <th>Nb</th>
                <th>Couleur</th>
                <th>Prix</th>
                <th>Dimension</th>
                <th>actions</th>
                
            </tr>
        </thead>
        <tbody>
        {% for oeuvre in oeuvres %}
            <tr>
                <td>{{ oeuvre.id }}</td>
                <td>{{ oeuvre.type }}</td>
                <td>{{ oeuvre.nb }}</td>
                <td>{{ oeuvre.couleur }}</td>
                <td>{{ oeuvre.prix }}</td>
                <td>{{ oeuvre.dimension }}</td>
                <td>
                    <a href="{{ path('app_oeuvre_showF', {'id': oeuvre.id}) }}">show</a>
                    
                </td>
                
            </tr>
       
        {% endfor %}
        </tbody>
    </table>

    
{% endblock %}