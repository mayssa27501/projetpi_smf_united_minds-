{% extends 'layoutFrontOffice.html.twig' %}


{% block title %}Commande index{% endblock %}

{% block body %}
<li class="nav-item"><a href="{{path('app_oeuvre_Front')}}" class="nav-link">Liste Oeuvres</a></li>
    <h1>Commande index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Adresse</th>
                <th>Quantite</th>
                <th>Prixc</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for commande in commandes %}
            <tr>
                <td>{{ commande.id }}</td>
                <td>{{ commande.adresse }}</td>
                <td>{{ commande.quantite }}</td>
                <td>{{ commande.prixc }}</td>
                <td>
                    <a href="{{ path('app_commande_showF', {'id': commande.id}) }}">show</a>
                    
                </td>
            </tr>
       
        {% endfor %}
        </tbody>
    </table>

    
{% endblock %}